    }

    .video-slider-5__name {
      font-size: 15px;
    }

    .video-slider-5__product {
      font-size: 13px;
    }

    .video-slider-5__star {
      width: 15px;
      height: 15px;
    }

    .video-slider-5__nav {
      width: 44px;
      height: 44px;
    }
  }

  /* Large Mobile (550px+) */
  @media (min-width: 550px) {
    .video-slider-5 {
      padding: 60px 0;
    }

    .video-slider-5__header {
      margin-bottom: 40px;
    }

    .video-slider-5__label {
      font-size: 12px;
