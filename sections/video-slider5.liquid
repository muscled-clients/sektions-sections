<section class="video-slider-5" data-section-id="{{ section.id }}">
  <!-- Header -->
  <div class="video-slider-5__header">
    {%- if section.settings.label != blank -%}
      <p class="video-slider-5__label">{{ section.settings.label }}</p>
    {%- endif -%}
    {%- if section.settings.heading != blank -%}
      <h2 class="video-slider-5__heading">{{ section.settings.heading }}</h2>
    {%- endif -%}
    {%- if section.settings.subtitle != blank -%}
      <p class="video-slider-5__subtitle">{{ section.settings.subtitle }}</p>
    {%- endif -%}
  </div>

  <!-- Slider -->
  <div class="video-slider-5__slider-wrapper">
    <div class="video-slider-5__swiper swiper">
      <div class="swiper-wrapper">
        {%- for block in section.blocks -%}
          <div class="video-slider-5__slide swiper-slide" {{ block.shopify_attributes }}>
            <div class="video-slider-5__card" data-card-index="{{ forloop.index0 }}">
              <!-- Video -->
              {%- if block.settings.video != blank -%}
                {{ block.settings.video | video_tag: autoplay: false, loop: true, muted: false, controls: false, class: 'video-slider-5__media', playsinline: true }}
              {%- endif -%}

              <!-- Poster/Thumbnail -->
              {%- if block.settings.poster != blank -%}
                {{ block.settings.poster | image_url: width: 800 | image_tag: class: 'video-slider-5__poster', loading: 'lazy' }}
              {%- else -%}
                <div class="video-slider-5__poster" style="background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);"></div>
              {%- endif -%}

              <!-- Play Button -->
              <button class="video-slider-5__play-btn" aria-label="Play video">
                <svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                  <path d="M8 5v14l11-7z"/>
                </svg>
              </button>

              <!-- Content Overlay -->
              <div class="video-slider-5__content">
                {%- if block.settings.name != blank -%}
                  <p class="video-slider-5__name">{{ block.settings.name }}</p>
                {%- endif -%}
                {%- if block.settings.product_name != blank -%}
                  <p class="video-slider-5__product">{{ block.settings.product_name }}</p>
                {%- endif -%}
                {%- if block.settings.show_stars -%}
                  <div class="video-slider-5__stars">
                    {%- for i in (1..5) -%}
                      <svg class="video-slider-5__star" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                        <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                      </svg>
                    {%- endfor -%}
                  </div>
                {%- endif -%}
              </div>
            </div>
          </div>
        {%- endfor -%}
      </div>
    </div>

    <!-- Navigation Arrows -->
    <button class="video-slider-5__nav video-slider-5__nav--prev" aria-label="Previous slide">
      <svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
        <polyline points="15 18 9 12 15 6"/>
      </svg>
    </button>
    <button class="video-slider-5__nav video-slider-5__nav--next" aria-label="Next slide">
      <svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
        <polyline points="9 18 15 12 9 6"/>
      </svg>
    </button>
  </div>
</section>

<!-- Swiper JS -->
<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    const sectionId = '{{ section.id }}';
    const section = document.querySelector(`[data-section-id="${sectionId}"]`);
    if (!section) return;

    const swiperEl = section.querySelector('.video-slider-5__swiper');
    const prevBtn = section.querySelector('.video-slider-5__nav--prev');
    const nextBtn = section.querySelector('.video-slider-5__nav--next');

    // Initialize Swiper with coverflow effect
    const swiper = new Swiper(swiperEl, {
      effect: 'coverflow',
      grabCursor: true,
      centeredSlides: true,
      loop: true,
      speed: 400,
      slidesPerView: 'auto',
      coverflowEffect: {
        rotate: 0,
        stretch: 0,
        depth: 200,
        modifier: 1,
        slideShadows: false,
      },
      breakpoints: {
        550: {
          coverflowEffect: {
            depth: 250,
          }
        },
        768: {
          coverflowEffect: {
            depth: 300,
          }
        },
        1024: {
          coverflowEffect: {
            depth: 350,
          }
        },
        1400: {
          coverflowEffect: {
            depth: 400,
          }
        }
      }
    });

    // Navigation buttons
    prevBtn.addEventListener('click', () => swiper.slidePrev());
    nextBtn.addEventListener('click', () => swiper.slideNext());

    // Video playback management
    const cards = section.querySelectorAll('.video-slider-5__card');
    let currentlyPlaying = null;

    cards.forEach((card) => {
      const playBtn = card.querySelector('.video-slider-5__play-btn');
      const video = card.querySelector('video');
      const poster = card.querySelector('.video-slider-5__poster');

      if (!playBtn || !video) return;

      playBtn.addEventListener('click', function(e) {
        e.stopPropagation();

        // Pause any currently playing video
        if (currentlyPlaying && currentlyPlaying !== card) {
          const prevVideo = currentlyPlaying.querySelector('video');
          if (prevVideo) {
            prevVideo.pause();
            prevVideo.currentTime = 0;
          }
          currentlyPlaying.classList.remove('is-playing');
        }

        // Toggle current video
        if (card.classList.contains('is-playing')) {
          video.pause();
          card.classList.remove('is-playing');
          currentlyPlaying = null;
        } else {
          video.play().catch(err => console.log('Video play failed:', err));
          card.classList.add('is-playing');
          currentlyPlaying = card;
        }
      });

      // When video ends, reset state
      video.addEventListener('ended', function() {
        card.classList.remove('is-playing');
        video.currentTime = 0;
        currentlyPlaying = null;
      });

      // Click on card (not play button) to pause
      card.addEventListener('click', function(e) {
        if (e.target === playBtn || playBtn.contains(e.target)) return;

        if (card.classList.contains('is-playing')) {
          video.pause();
          card.classList.remove('is-playing');
          currentlyPlaying = null;
        }
      });
    });

    // Pause video when slide changes
    swiper.on('slideChange', function() {
      if (currentlyPlaying) {
        const video = currentlyPlaying.querySelector('video');
        if (video) {
          video.pause();
          video.currentTime = 0;
        }
        currentlyPlaying.classList.remove('is-playing');
        currentlyPlaying = null;
      }
    });
  });
</script>

