{% style %}
  #shopify-section-{{ section.id }} {
    --vs5-bg: {{ section.settings.background_color }};
    --vs5-text: {{ section.settings.text_color }};
    --vs5-label: {{ section.settings.label_color }};
    --vs5-subtitle: {{ section.settings.subtitle_color }};
    --vs5-card-text: {{ section.settings.card_text_color }};
    --vs5-star: {{ section.settings.star_color }};
    --vs5-btn-bg: {{ section.settings.nav_button_bg }};
    --vs5-btn-icon: {{ section.settings.nav_button_icon }};
    display: block;
    width: 100%;
    max-width: 100%;
    margin-top: {{ section.settings.margin_top | times: 0.7 | round }}px;
    margin-bottom: {{ section.settings.margin_bottom | times: 0.7 | round }}px;
  }
  @media (min-width: 550px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.margin_top | times: 0.8 | round }}px;
      margin-bottom: {{ section.settings.margin_bottom | times: 0.8 | round }}px;
    }
  }
  @media (min-width: 768px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.margin_top | times: 0.9 | round }}px;
      margin-bottom: {{ section.settings.margin_bottom | times: 0.9 | round }}px;
    }
  }
  @media (min-width: 1024px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.margin_top }}px;
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }
{% endstyle %}

<!-- Google Fonts -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@400;500;600;700&family=Inter:wght@400;500;600&display=swap" rel="stylesheet">

<!-- Swiper CSS -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />

<style>
  /* Box-sizing */
  .video-slider-5,
  .video-slider-5 *,
  .video-slider-5 *::before,
  .video-slider-5 *::after {
    box-sizing: border-box;
  }

  /* Base - Small Mobile (< 400px) */
  .video-slider-5 {
    background: var(--vs5-bg, #ffffff);
    padding: 40px 0;
    font-family: 'Inter', sans-serif;
    overflow-x: clip;
    overflow-y: visible;
    position: relative;
  }

  .video-slider-5__header {
    text-align: center;
    padding: 0 20px;
    margin-bottom: 30px;
  }

  .video-slider-5__label {
    font-size: 11px;
    font-weight: 500;
    letter-spacing: 2px;
    text-transform: uppercase;
    color: var(--vs5-label, #666666);
    margin: 0 0 12px;
  }

  .video-slider-5__heading {
    font-family: 'Cormorant Garamond', serif;
    font-size: 28px;
    font-weight: 700;
    line-height: 1.2;
    color: var(--vs5-text, #000000);
    margin: 0 0 12px;
  }

  .video-slider-5__subtitle {
    font-size: 14px;
    font-weight: 400;
    line-height: 1.5;
    color: var(--vs5-subtitle, #666666);
    margin: 0;
    max-width: 500px;
    margin-left: auto;
    margin-right: auto;
  }

  /* Slider Container */
  .video-slider-5__slider-wrapper {
    position: relative;
    padding: 0 40px;
    overflow: visible;
  }

  .video-slider-5__swiper {
    overflow: visible !important;
    padding: 20px 0;
  }

  .video-slider-5__swiper .swiper-wrapper {
    align-items: center;
    display: flex !important;
  }

  /* Force Swiper to use auto width - critical for coverflow */
  .video-slider-5__swiper.swiper {
    overflow: visible !important;
  }

  .video-slider-5 .swiper-wrapper {
    transform-style: preserve-3d;
  }

  .video-slider-5 .swiper-slide,
  .video-slider-5__slide,
  .video-slider-5__slide.swiper-slide,
  .video-slider-5__swiper .swiper-slide {
    width: 180px !important;
    max-width: 180px !important;
    flex-shrink: 0 !important;
    transition: all 0.3s ease;
  }

  /* Card */
  .video-slider-5__card {
    position: relative;
    aspect-ratio: 9 / 12;
    border-radius: 12px;
    overflow: hidden;
    background: #f0f0f0;
    box-shadow: 0 10px 40px rgba(0, 0, 0, 0.15);
  }

  .video-slider-5__media {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .video-slider-5__poster {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    z-index: 1;
    transition: opacity 0.3s ease;
  }

  .video-slider-5__card.is-playing .video-slider-5__poster {
    opacity: 0;
    pointer-events: none;
  }

  /* Play Button */
  .video-slider-5__play-btn {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 50px;
    height: 50px;
    background: rgba(255, 255, 255, 0.25);
    border: 1px solid rgba(255, 255, 255, 0.6);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    z-index: 5;
    transition: all 0.3s ease;
    backdrop-filter: blur(4px);
  }

  .video-slider-5__play-btn:hover {
    background: rgba(255, 255, 255, 0.35);
    transform: translate(-50%, -50%) scale(1.05);
  }

  .video-slider-5__play-btn svg {
    width: 18px;
    height: 18px;
    fill: #ffffff;
    margin-left: 3px;
  }

  .video-slider-5__card.is-playing .video-slider-5__play-btn {
    opacity: 0;
    pointer-events: none;
  }

  /* Content Overlay */
  .video-slider-5__content {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 60px 16px 16px;
    background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.7) 100%);
    z-index: 3;
  }

  .video-slider-5__name {
    font-size: 14px;
    font-weight: 600;
    color: var(--vs5-card-text, #ffffff);
    margin: 0 0 4px;
  }

  .video-slider-5__product {
    font-size: 12px;
    font-weight: 400;
    font-style: italic;
    color: var(--vs5-card-text, #ffffff);
    opacity: 0.9;
    margin: 0 0 8px;
  }

  .video-slider-5__stars {
    display: flex;
    gap: 2px;
  }

  .video-slider-5__star {
    width: 14px;
    height: 14px;
    fill: var(--vs5-star, #f5a623);
  }

  /* Navigation Arrows */
  .video-slider-5__nav {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: 40px;
    height: 40px;
    background: var(--vs5-btn-bg, #ffffff);
    border: none;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    z-index: 20;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.15);
    transition: all 0.3s ease;
  }

  .video-slider-5__nav:hover {
    transform: translateY(-50%) scale(1.08);
    box-shadow: 0 6px 20px rgba(0, 0, 0, 0.2);
  }

  .video-slider-5__nav:disabled {
    opacity: 0.5;
    cursor: not-allowed;
  }

  .video-slider-5__nav--prev {
    left: 5px;
  }

  .video-slider-5__nav--next {
    right: 5px;
  }

  .video-slider-5__nav svg {
    width: 16px;
    height: 16px;
    stroke: var(--vs5-btn-icon, #000000);
    stroke-width: 2;
    fill: none;
  }

  /* Swiper slide active state - handled by coverflow */
  .video-slider-5__swiper .swiper-slide-active .video-slider-5__card {
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.25);
  }

  /* Medium Mobile (400px+) */
  @media (min-width: 400px) {
    .video-slider-5 {
      padding: 50px 0;
    }

    .video-slider-5__header {
      margin-bottom: 36px;
    }

    .video-slider-5__label {
      font-size: 12px;
