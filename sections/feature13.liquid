              </div>
              {% if block.settings.title != blank %}
                <h3 class="f13__title">{{ block.settings.title }}</h3>
              {% endif %}
              {% if block.settings.description != blank %}
                <p class="f13__description">{{ block.settings.description }}</p>
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
</section>

<style>
  /* Base Section */
  .feature-13 {
    display: block;
    width: 100%;
    max-width: 100%;
    box-sizing: border-box;
    background-color: var(--f13-bg);
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
    padding: 48px 20px;
    margin-top: {{ section.settings.margin_top }}px;
    margin-bottom: {{ section.settings.margin_bottom }}px;
    overflow: hidden;
    position: relative;
  }

  .feature-13 *,
  .feature-13 *::before,
  .feature-13 *::after {
    box-sizing: border-box;
  }

  .feature-13 img {
    max-width: 100%;
    height: auto;
    border: 0;
    vertical-align: middle;
  }

  .feature-13 h2,
  .feature-13 h3,
  .feature-13 p {
    margin: 0;
    padding: 0;
  }

  /* Container */
  .f13__container {
    display: block;
    width: 100%;
    max-width: 1200px;
    margin: 0 auto;
  }

  /* Heading */
  .f13__heading {
    font-family: 'Playfair Display', 'Georgia', serif;
    font-size: 32px;
    font-weight: 400;
    font-style: italic;
    color: var(--f13-heading-color);
    text-align: center;
    margin-bottom: 48px;
    line-height: 1.2;
  }

  /* Grid - Mobile First (stacked) */
  .f13__grid {
    display: flex;
    flex-direction: column;
    gap: 40px;
    align-items: center;
  }

  /* Features Container */
  .f13__features {
    display: flex;
    flex-direction: column;
    gap: 36px;
    width: 100%;
    max-width: 320px;
  }

  /* Individual Feature */
  .f13__feature {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
  }

  /* Icon */
  .f13__icon {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 40px;
    height: 40px;
    margin-bottom: 14px;
    color: var(--f13-icon-color);
  }

  .f13__icon svg {
    display: block;
    width: 100%;
    height: 100%;
  }

  /* Title */
  .f13__title {
    font-size: 13px;
    font-weight: 600;
    color: var(--f13-title-color);
    text-transform: uppercase;
    letter-spacing: 0.12em;
    margin-bottom: 10px;
    line-height: 1.3;
  }

  /* Description */
  .f13__description {
    font-size: 13px;
    font-weight: 400;
    color: var(--f13-text-color);
    line-height: 1.6;
    max-width: 220px;
  }

  /* Image Wrapper */
  .f13__image-wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    max-width: 400px;
    order: -1;
  }

  .f13__image {
    display: block;
    width: 100%;
    height: auto;
    object-fit: contain;
  }

  .f13__image-placeholder {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    aspect-ratio: 9 / 11;
    background-color: #f0f0f0;
    border-radius: 8px;
  }

  .f13__image-placeholder svg {
    display: block;
    width: 60%;
    height: auto;
    opacity: 0.4;
  }

  /* ========================================
     RESPONSIVE BREAKPOINTS
     ======================================== */

  /* Small Mobile (< 400px) */
  @media screen and (max-width: 399px) {
    .feature-13 {
      padding: 36px 16px;
    }

    .f13__heading {
      font-size: 26px;
      margin-bottom: 36px;
    }

    .f13__grid {
      gap: 32px;
    }

    .f13__features {
      gap: 28px;
    }

    .f13__icon {
      width: 36px;
      height: 36px;
      margin-bottom: 12px;
    }

    .f13__title {
      font-size: 12px;
      margin-bottom: 8px;
    }

    .f13__description {
      font-size: 12px;
      max-width: 200px;
    }

    .f13__image-wrapper {
      max-width: 280px;
    }
  }

  /* Medium Mobile (400px+) */
  @media screen and (min-width: 400px) {
    .feature-13 {
      padding: 56px 24px;
    }

    .f13__heading {
      font-size: 36px;
      margin-bottom: 48px;
    }

    .f13__features {
      flex-direction: row;
      flex-wrap: wrap;
      justify-content: center;
      gap: 24px 32px;
      max-width: 100%;
    }

    .f13__feature {
      width: calc(50% - 16px);
      max-width: 180px;
    }

    .f13__image-wrapper {
      max-width: 350px;
    }
  }

  /* Large Mobile (550px+) */
  @media screen and (min-width: 550px) {
    .feature-13 {
      padding: 64px 32px;
    }

    .f13__heading {
      font-size: 40px;
      margin-bottom: 56px;
    }

    .f13__grid {
      gap: 48px;
    }

    .f13__features {
      gap: 32px 40px;
    }

    .f13__feature {
      max-width: 200px;
    }

    .f13__icon {
      width: 44px;
      height: 44px;
    }

    .f13__title {
      font-size: 14px;
    }

    .f13__description {
      font-size: 14px;
      max-width: 200px;
    }

    .f13__image-wrapper {
      max-width: 400px;
    }
  }

  /* Tablet (768px+) - 3 column layout */
  @media screen and (min-width: 768px) {
    .feature-13 {
      padding: 72px 40px;
    }

    .f13__heading {
      font-size: 44px;
      margin-bottom: 64px;
    }

    .f13__grid {
      display: grid;
      grid-template-columns: 1fr 1.2fr 1fr;
      gap: 40px;
      align-items: center;
    }

    .f13__features {
      flex-direction: column;
      justify-content: space-between;
      gap: 48px;
      max-width: none;
    }

    .f13__feature {
      width: 100%;
      max-width: none;
    }

    .f13__features--left .f13__feature {
      align-items: center;
    }

    .f13__features--right .f13__feature {
      align-items: center;
    }

    .f13__image-wrapper {
      order: 0;
      max-width: none;
    }

    .f13__description {
      max-width: 220px;
    }
  }

  /* Desktop (1024px+) */
  @media screen and (min-width: 1024px) {
    .feature-13 {
      padding: 80px 50px;
    }

    .f13__heading {
      font-size: 48px;
      margin-bottom: 72px;
    }

    .f13__grid {
      grid-template-columns: 1fr 1.4fr 1fr;
      gap: 60px;
    }

    .f13__features {
      gap: 56px;
    }

    .f13__icon {
      width: 48px;
      height: 48px;
      margin-bottom: 16px;
    }

    .f13__title {
      font-size: 14px;
      letter-spacing: 0.15em;
      margin-bottom: 12px;
    }

    .f13__description {
      font-size: 14px;
      line-height: 1.7;
      max-width: 240px;
    }
  }

  /* Large Desktop (1400px+) */
  @media screen and (min-width: 1400px) {
    .feature-13 {
      padding: 100px 60px;
    }

    .f13__container {
      max-width: 1300px;
    }

    .f13__heading {
      font-size: 52px;
      margin-bottom: 80px;
    }

    .f13__grid {
      gap: 80px;
    }

    .f13__features {
      gap: 64px;
    }

    .f13__icon {
      width: 52px;
      height: 52px;
    }

    .f13__title {
      font-size: 15px;
    }

    .f13__description {
      font-size: 15px;
      max-width: 260px;
    }
  }
</style>

{% schema %}
{
  "name": "SEK Feature 13",
  "tag": "section",
  "class": "feature-13-section",
  "settings": [
    {
      "type": "header",
      "content": "Content"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Why we are better"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Center Image",
      "info": "Product or feature image displayed in the center"
    },
    {
      "type": "header",
      "content": "Colors"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background Color",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "heading_color",
      "label": "Heading Color",
      "default": "#1a1a2e"
    },
    {
      "type": "color",
      "id": "title_color",
      "label": "Feature Title Color",
      "default": "#1a1a2e"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Description Color",
      "default": "#6b6b7b"
    },
    {
      "type": "color",
      "id": "icon_color",
      "label": "Icon Color",
      "default": "#1a1a2e"
    },
    {
      "type": "header",
      "content": "Section Spacing"
    },
    {
      "type": "range",
      "id": "margin_top",
      "label": "Margin Top",
      "min": 0,
      "max": 100,
      "step": 5,
      "default": 0,
      "unit": "px"
    },
    {
      "type": "range",
      "id": "margin_bottom",
      "label": "Margin Bottom",
      "min": 0,
      "max": 100,
      "step": 5,
      "default": 0,
      "unit": "px"
    }
  ],
  "blocks": [
    {
      "type": "feature_left",
      "name": "Left Feature",
      "settings": [
        {
          "type": "select",
          "id": "icon",
          "label": "Icon",
          "default": "heart",
          "options": [
            { "value": "heart", "label": "Heart" },
            { "value": "clock", "label": "Clock" },
