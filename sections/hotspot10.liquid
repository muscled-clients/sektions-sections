    }

    .hotspots-10__grid {
      grid-template-columns: 1fr 1fr;
      gap: 50px;
    }

    .hotspots-10__content {
      order: {% if section.settings.image_position == 'right' %}1{% else %}2{% endif %};
    }

    .hotspots-10__image-container {
