      margin-bottom: 16px;
    }

    .hotspots-10__features {
      gap: 10px;
      margin-bottom: 18px;
    }

    .hotspots-10__feature {
      gap: 9px;
      font-size: 14px;
    }

    .hotspots-10__feature-icon {
      width: 17px;
      height: 17px;
    }

    .hotspots-10__description {
      font-size: 14px;
      margin-bottom: 22px;
    }

    .hotspots-10__button {
      gap: 11px;
      padding: 12px 26px;
      font-size: 12px;
    }

    .hotspots-10__button-arrow {
      width: 15px;
      height: 15px;
    }

    .hotspots-10__hotspot-dot {
      width: 30px;
      height: 30px;
    }

    .hotspots-10__hotspot-icon {
      width: 15px;
      height: 15px;
    }

    .hotspots-10__popup {
