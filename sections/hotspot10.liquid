  /* 768px - Tablet */
  @media (min-width: 768px) {
    .hotspots-10 {
      padding: 50px 28px;
    }

    .hotspots-10__grid {
      gap: 38px;
    }

    .hotspots-10__title {
      font-size: 30px;
      margin-bottom: 20px;
    }

    .hotspots-10__features {
      gap: 12px;
      margin-bottom: 22px;
    }

    .hotspots-10__feature {
