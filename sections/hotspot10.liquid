</style>

<section class="hotspots-10" data-section-id="{{ section.id }}">
  <div class="hotspots-10__container">
    <div class="hotspots-10__grid">
      <!-- Content -->
      <div class="hotspots-10__content">
        {%- if section.settings.title != blank -%}
          <h2 class="hotspots-10__title">{{ section.settings.title }}</h2>
        {%- endif -%}

        {%- assign feature_blocks = section.blocks | where: "type", "feature" -%}
        {%- if feature_blocks.size > 0 -%}
          <div class="hotspots-10__features">
            {%- for block in feature_blocks -%}
              <div class="hotspots-10__feature" {{ block.shopify_attributes }}>
                <svg class="hotspots-10__feature-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3">
                  <polyline points="20 6 9 17 4 12"/>
                </svg>
                <span>{{ block.settings.text }}</span>
              </div>
            {%- endfor -%}
          </div>
        {%- endif -%}

        {%- if section.settings.description != blank -%}
          <div class="hotspots-10__description">{{ section.settings.description }}</div>
        {%- endif -%}

        {%- if section.settings.button_text != blank -%}
          <a href="{{ section.settings.button_link }}" class="hotspots-10__button">
            <span>{{ section.settings.button_text }}</span>
            <svg class="hotspots-10__button-arrow" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <line x1="5" y1="12" x2="19" y2="12"/>
              <polyline points="12 5 19 12 12 19"/>
            </svg>
          </a>
        {%- endif -%}
      </div>

      <!-- Image with Hotspots -->
      <div class="hotspots-10__image-container">
        <div class="hotspots-10__image-wrapper">
          <div class="hotspots-10__image">
            {%- if section.settings.image != blank -%}
              {{ section.settings.image | image_url: width: 1200 | image_tag: loading: 'eager', alt: section.settings.title }}
            {%- else -%}
              <div class="hotspots-10__image-placeholder">
                <span>Add image</span>
              </div>
            {%- endif -%}
          </div>

          <!-- Hotspots -->
          {%- assign hotspot_blocks = section.blocks | where: "type", "hotspot" -%}
          {%- for block in hotspot_blocks -%}
            <div class="hotspots-10__hotspot"
                 style="top: {{ block.settings.position_top }}%; left: {{ block.settings.position_left }}%;"
                 data-hotspot-id="{{ block.id }}"
                 {{ block.shopify_attributes }}>
              <div class="hotspots-10__hotspot-dot">
                <svg class="hotspots-10__hotspot-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor">
                  <line x1="12" y1="5" x2="12" y2="19"/>
                  <line x1="5" y1="12" x2="19" y2="12"/>
                </svg>
              </div>

              {%- if block.settings.product_title != blank -%}
                <div class="hotspots-10__popup">
                  <div class="hotspots-10__popup-content">
                    {%- if block.settings.product_image != blank -%}
                      <div class="hotspots-10__popup-image">
                        {{ block.settings.product_image | image_url: width: 160 | image_tag: loading: 'lazy', alt: block.settings.product_title }}
                      </div>
                    {%- endif -%}

                    <div class="hotspots-10__popup-info">
                      <h3 class="hotspots-10__popup-title">{{ block.settings.product_title }}</h3>

                      {%- if block.settings.product_description != blank -%}
                        <p class="hotspots-10__popup-description">{{ block.settings.product_description }}</p>
                      {%- endif -%}

                      {%- if block.settings.product_price != blank -%}
                        <div class="hotspots-10__popup-price">{{ block.settings.product_price }}</div>
                      {%- endif -%}

                      {%- if block.settings.product_link != blank -%}
                        <a href="{{ block.settings.product_link }}" class="hotspots-10__popup-link">View</a>
                      {%- endif -%}
                    </div>
                  </div>
                </div>
              {%- endif -%}
            </div>
          {%- endfor -%}
        </div>
      </div>
    </div>
  </div>
</section>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    const sectionId = '{{ section.id }}';
    const section = document.querySelector(`[data-section-id="${sectionId}"]`);
    if (!section) return;

    const hotspots = section.querySelectorAll('.hotspots-10__hotspot');

    hotspots.forEach(hotspot => {
      const dot = hotspot.querySelector('.hotspots-10__hotspot-dot');

      dot.addEventListener('click', function(e) {
        e.stopPropagation();

        // Close all other hotspots
        hotspots.forEach(h => {
          if (h !== hotspot) {
            h.classList.remove('active');
          }
        });

        // Toggle current hotspot
        hotspot.classList.toggle('active');
      });
    });

    // Close hotspots when clicking outside
    document.addEventListener('click', function(e) {
      if (!e.target.closest('.hotspots-10__hotspot')) {
        hotspots.forEach(h => h.classList.remove('active'));
      }
    });
  });
</script>

