{% style %}
  #shopify-section-{{ section.id }} {
    --h10-bg: {{ section.settings.background_color }};
    --h10-text: {{ section.settings.text_color }};
    --h10-heading: {{ section.settings.heading_color }};
    --h10-hotspot-bg: {{ section.settings.hotspot_background }};
    --h10-hotspot-text: {{ section.settings.hotspot_text_color }};
    --h10-popup-bg: {{ section.settings.popup_background }};
    --h10-btn-bg: {{ section.settings.button_background }};
    --h10-btn-text: {{ section.settings.button_text_color }};
    --h10-border-radius: {{ section.settings.image_border_radius }}px;
    display: block;
    width: 100%;
    max-width: 100%;
    overflow: hidden;
    margin-top: {{ section.settings.margin_top | times: 0.7 }}px;
    margin-bottom: {{ section.settings.margin_bottom | times: 0.7 }}px;
  }

  @media (min-width: 550px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.margin_top | times: 0.8 }}px;
      margin-bottom: {{ section.settings.margin_bottom | times: 0.8 }}px;
    }
  }

  @media (min-width: 768px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.margin_top | times: 0.9 }}px;
      margin-bottom: {{ section.settings.margin_bottom | times: 0.9 }}px;
    }
  }

  @media (min-width: 1024px) {
    #shopify-section-{{ section.id }} {
      margin-top: {{ section.settings.margin_top }}px;
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }
{% endstyle %}

<style>
  @import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&family=Open+Sans:wght@400;600;700&display=swap');

  .hotspots-10 *,
  .hotspots-10 *::before,
  .hotspots-10 *::after {
    box-sizing: border-box;
  }

  .hotspots-10 {
    display: block;
    width: 100%;
    background: var(--h10-bg, #ffffff);
    padding: 28px 12px;
    font-family: 'Open Sans', sans-serif;
  }

  .hotspots-10__container {
    max-width: 1400px;
    margin: 0 auto;
  }

  .hotspots-10__grid {
    display: grid;
    grid-template-columns: 1fr;
    gap: 28px;
    align-items: center;
  }

  /* Content - Mobile First */
  .hotspots-10__content {
    order: 2;
  }

  .hotspots-10__title {
    font-family: 'Montserrat', sans-serif;
    font-size: 22px;
    font-weight: 700;
    color: var(--h10-heading, #121212);
    margin: 0 0 14px;
    line-height: 1.2;
  }

  .hotspots-10__features {
    display: flex;
    flex-direction: column;
    gap: 8px;
    margin-bottom: 16px;
  }

  .hotspots-10__feature {
    display: flex;
    align-items: center;
    gap: 8px;
    font-size: 13px;
    font-weight: 600;
    color: var(--h10-text, #121212);
  }

  .hotspots-10__feature-icon {
    width: 16px;
    height: 16px;
    color: #00b280;
    flex-shrink: 0;
  }

  .hotspots-10__description {
    font-size: 13px;
    font-weight: 400;
    color: var(--h10-text, #121212);
    line-height: 1.6;
    margin: 0 0 20px;
  }

  .hotspots-10__button {
    display: inline-flex;
    align-items: center;
    gap: 10px;
    background: var(--h10-btn-bg, #cbe514);
    color: var(--h10-btn-text, #121212);
    padding: 11px 24px;
    border-radius: 4px;
    font-size: 11px;
    font-weight: 700;
    text-transform: uppercase;
    text-decoration: none;
    letter-spacing: 0.5px;
    transition: all 0.25s ease;
  }

  .hotspots-10__button:hover {
    opacity: 0.9;
    transform: translateY(-2px);
  }

  .hotspots-10__button-arrow {
    width: 14px;
    height: 14px;
  }

  /* Image Container - Mobile First */
  .hotspots-10__image-container {
    position: relative;
    order: 1;
  }

  .hotspots-10__image-wrapper {
    position: relative;
    width: 100%;
    padding-bottom: 100%;
    overflow: hidden;
    border-radius: var(--h10-border-radius, 20px);
  }

  .hotspots-10__image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }

  .hotspots-10__image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hotspots-10__image-placeholder {
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, #f5f5f5 0%, #e0e0e0 100%);
    display: flex;
    align-items: center;
    justify-content: center;
    color: #999;
    font-size: 13px;
  }

  /* Hotspots - Mobile First */
  .hotspots-10__hotspot {
    position: absolute;
    z-index: 10;
  }

  .hotspots-10__hotspot-dot {
    width: 28px;
    height: 28px;
    background: var(--h10-hotspot-bg, #cbe514);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.25s ease;
    box-shadow: 0 3px 10px rgba(0, 0, 0, 0.15);
  }

  .hotspots-10__hotspot-dot:hover {
    background: #c3dc0b;
    transform: scale(1.1);
  }

  .hotspots-10__hotspot-icon {
    width: 14px;
    height: 14px;
    color: var(--h10-hotspot-text, #121212);
    stroke-width: 2.5;
  }

  /* Popup - Mobile First */
  .hotspots-10__popup {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: var(--h10-popup-bg, #ffffff);
    border-radius: 10px;
    padding: 12px;
    min-width: 200px;
    max-width: 240px;
    box-shadow: 0 6px 20px rgba(0, 0, 0, 0.2);
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.3s ease, visibility 0.3s ease;
    z-index: 20;
    pointer-events: none;
  }

  .hotspots-10__hotspot.active .hotspots-10__popup {
    opacity: 1;
    visibility: visible;
    pointer-events: auto;
  }

  .hotspots-10__popup-content {
    display: flex;
    gap: 10px;
  }

  .hotspots-10__popup-image {
    width: 60px;
    height: 60px;
    flex-shrink: 0;
    border-radius: 6px;
    overflow: hidden;
  }

  .hotspots-10__popup-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .hotspots-10__popup-info {
    flex: 1;
    min-width: 0;
  }

  .hotspots-10__popup-title {
    font-size: 12px;
    font-weight: 700;
    color: #121212;
    margin: 0 0 3px;
    line-height: 1.3;
  }

  .hotspots-10__popup-description {
    font-size: 10px;
    font-weight: 400;
    color: #666;
    margin: 0 0 6px;
    line-height: 1.4;
  }

  .hotspots-10__popup-price {
    font-size: 13px;
    font-weight: 700;
    color: #121212;
    margin: 0 0 8px;
  }

  .hotspots-10__popup-link {
    display: inline-block;
    background: #121212;
    color: #ffffff;
    padding: 6px 12px;
    border-radius: 4px;
    font-size: 10px;
    font-weight: 700;
    text-transform: uppercase;
    text-decoration: none;
    letter-spacing: 0.5px;
    transition: all 0.25s ease;
  }

  .hotspots-10__popup-link:hover {
    background: #2d2d2d;
  }

  /* ============================================
     RESPONSIVE BREAKPOINTS
     ============================================ */

