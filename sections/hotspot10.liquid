      right: calc(100% + 20px);
      top: 50%;
      transform: translateY(-50%);
      {% else %}
      left: calc(100% + 20px);
      top: 50%;
      transform: translateY(-50%);
      {% endif %}
    }

    .hotspots-10__popup-image {
      width: 80px;
      height: 80px;
    }
  }

  /* 1400px - Large Desktop */
  @media (min-width: 1400px) {
