<!-- FAQ + CTA Section -->
<style>
  #faq-cta-{{ section.id }} {
    padding: {{ section.settings.padding_top }}px 0 {{ section.settings.padding_bottom }}px 0;
    background-color: {{ section.settings.outer_background_color }};
  }

  #faq-cta-{{ section.id }} .faq-cta-container {
    max-width: {{ section.settings.max_width }}px;
    margin: 0 auto;
    padding: 0 {{ section.settings.container_padding }}px;
  }

  #faq-cta-{{ section.id }} .faq-cta-wrapper {
    background-color: {{ section.settings.section_background_color }};
    border-radius: {{ section.settings.section_border_radius }}px;
    padding: {{ section.settings.section_padding }}px;
    display: grid;
    grid-template-columns: {{ section.settings.left_column_width }}% 1fr;
    gap: {{ section.settings.column_gap }}px;
    align-items: start;
  }

  /* Left Column Styles */
  #faq-cta-{{ section.id }} .faq-left-column {
    padding-right: {{ section.settings.left_padding_right }}px;
  }

  #faq-cta-{{ section.id }} .faq-heading {
    font-size: {{ section.settings.heading_size }}px;
    font-weight: {{ section.settings.heading_weight }};
    color: {{ section.settings.heading_color }};
    line-height: {{ section.settings.heading_line_height }};
    margin-bottom: {{ section.settings.heading_margin_bottom }}px;
  }

  #faq-cta-{{ section.id }} .faq-subheading {
    font-size: {{ section.settings.subheading_size }}px;
    color: {{ section.settings.subheading_color }};
    line-height: {{ section.settings.subheading_line_height }};
    margin-bottom: {{ section.settings.subheading_margin_bottom }}px;
  }

  #faq-cta-{{ section.id }} .faq-cta-button {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    padding: {{ section.settings.button_padding_y }}px {{ section.settings.button_padding_x }}px;
    background-color: {{ section.settings.button_background_color }};
    color: {{ section.settings.button_text_color }};
    border: {{ section.settings.button_border_width }}px solid {{ section.settings.button_border_color }};
    border-radius: {{ section.settings.button_border_radius }}px;
    font-size: {{ section.settings.button_font_size }}px;
    font-weight: {{ section.settings.button_font_weight }};
    text-decoration: none;
    transition: all 0.3s ease;
    cursor: pointer;
  }

  #faq-cta-{{ section.id }} .faq-cta-button:hover {
    background-color: {{ section.settings.button_hover_background }};
    color: {{ section.settings.button_hover_text_color }};
    transform: translateY(-2px);
  }

  #faq-cta-{{ section.id }} .button-arrow {
    transition: transform 0.3s ease;
  }

  #faq-cta-{{ section.id }} .faq-cta-button:hover .button-arrow {
    transform: translateX(4px);
  }

  /* Right Column - FAQ Accordion Styles */
  #faq-cta-{{ section.id }} .faq-right-column {
    display: flex;
    flex-direction: column;
    gap: {{ section.settings.faq_item_gap }}px;
  }

  #faq-cta-{{ section.id }} .faq-item {
    background-color: {{ section.settings.faq_item_background }};
    border-radius: {{ section.settings.faq_item_border_radius }}px;
    overflow: hidden;
    transition: all 0.3s ease;
    box-shadow: {{ section.settings.faq_item_shadow }};
  }

  #faq-cta-{{ section.id }} .faq-item:hover {
    box-shadow: {{ section.settings.faq_item_hover_shadow }};
  }

  #faq-cta-{{ section.id }} .faq-question {
    padding: {{ section.settings.faq_question_padding }}px;
    cursor: pointer;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: {{ section.settings.faq_question_size }}px;
    font-weight: {{ section.settings.faq_question_weight }};
    color: {{ section.settings.faq_question_color }};
    background-color: {{ section.settings.faq_question_background }};
    border: none;
    width: 100%;
    text-align: left;
    transition: all 0.3s ease;
  }

  #faq-cta-{{ section.id }} .faq-item.active .faq-question {
    font-weight: {{ section.settings.faq_question_active_weight }};
    color: {{ section.settings.faq_question_active_color }};
  }

  #faq-cta-{{ section.id }} .faq-chevron {
    width: {{ section.settings.chevron_size }}px;
    height: {{ section.settings.chevron_size }}px;
    transition: transform 0.3s ease;
    flex-shrink: 0;
    color: {{ section.settings.chevron_color }};
  }

  #faq-cta-{{ section.id }} .faq-item.active .faq-chevron {
    transform: rotate(180deg);
  }

  #faq-cta-{{ section.id }} .faq-answer-wrapper {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.4s ease;
  }

  #faq-cta-{{ section.id }} .faq-item.active .faq-answer-wrapper {
    max-height: 500px;
  }

  #faq-cta-{{ section.id }} .faq-answer {
    padding: 0 {{ section.settings.faq_answer_padding_x }}px {{ section.settings.faq_answer_padding_bottom }}px {{ section.settings.faq_answer_padding_x }}px;
    font-size: {{ section.settings.faq_answer_size }}px;
    line-height: {{ section.settings.faq_answer_line_height }};
    color: {{ section.settings.faq_answer_color }};
  }

  /* Mobile Responsive */
  @media (max-width: 768px) {
    #faq-cta-{{ section.id }} .faq-cta-wrapper {
      grid-template-columns: 1fr;
      padding: {{ section.settings.section_padding_mobile }}px;
      gap: 0;
    }

    #faq-cta-{{ section.id }} .faq-left-column {
      padding-right: 0;
      padding-bottom: {{ section.settings.mobile_content_padding_bottom }}px;
      margin-bottom: 0;
      text-align: center;
    }

    #faq-cta-{{ section.id }} .faq-right-column {
      padding-top: {{ section.settings.mobile_faq_padding_top }}px;
    }

    #faq-cta-{{ section.id }} .faq-cta-button {
      justify-content: center;
      margin-bottom: 0;
    }

    #faq-cta-{{ section.id }} .faq-heading {
      font-size: {{ section.settings.heading_size_mobile }}px;
    }

    #faq-cta-{{ section.id }} .faq-subheading {
      font-size: {{ section.settings.subheading_size_mobile }}px;
    }

    #faq-cta-{{ section.id }} .faq-question {
      font-size: {{ section.settings.faq_question_size_mobile }}px;
      padding: {{ section.settings.faq_question_padding_mobile }}px;
    }

    #faq-cta-{{ section.id }} .faq-answer {
      font-size: {{ section.settings.faq_answer_size_mobile }}px;
    }
  }

  /* Stacked Layout Option */
  {% if section.settings.layout == 'stacked' %}
    #faq-cta-{{ section.id }} .faq-cta-wrapper {
      grid-template-columns: 1fr;
    }

    #faq-cta-{{ section.id }} .faq-left-column {
      text-align: center;
      padding-right: 0;
      margin-bottom: {{ section.settings.stacked_gap }}px;
    }
  {% endif %}
</style>

<section id="faq-cta-{{ section.id }}">
  <div class="faq-cta-container">
    <div class="faq-cta-wrapper">
      <!-- Left Column -->
      <div class="faq-left-column">
        {% if section.settings.heading != blank %}
          <h2 class="faq-heading">{{ section.settings.heading }}</h2>
        {% endif %}

        {% if section.settings.subheading != blank %}
          <p class="faq-subheading">{{ section.settings.subheading }}</p>
        {% endif %}

        {% if section.settings.button_text != blank %}
          <a href="{{ section.settings.button_link | default: '#' }}" class="faq-cta-button">
            {{ section.settings.button_text }}
            {% if section.settings.show_button_arrow %}
              <svg class="button-arrow" width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M7 10H13M13 10L10 7M13 10L10 13" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
            {% endif %}
          </a>
        {% endif %}
      </div>

      <!-- Right Column - FAQ Accordion -->
      <div class="faq-right-column">
        {% for block in section.blocks %}
          {% if block.type == 'faq_item' %}
            <div class="faq-item" data-faq-item>
              <button class="faq-question" aria-expanded="false" aria-controls="faq-answer-{{ block.id }}">
                <span>{{ block.settings.question }}</span>
                <svg class="faq-chevron" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M6 9L12 15L18 9" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
              </button>
              <div class="faq-answer-wrapper" id="faq-answer-{{ block.id }}">
                <div class="faq-answer">
                  {{ block.settings.answer }}
                </div>
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
</section>

<script>
(function() {
  const sectionId = '{{ section.id }}';
  const allowMultiple = {{ section.settings.allow_multiple_open }};
  const section = document.getElementById(`faq-cta-${sectionId}`);

  if (!section) return;

  const faqItems = section.querySelectorAll('.faq-item');

  faqItems.forEach(item => {
    const question = item.querySelector('.faq-question');
    const answerWrapper = item.querySelector('.faq-answer-wrapper');

    question.addEventListener('click', () => {
      const isActive = item.classList.contains('active');

      // If not allowing multiple open, close all others
      if (!allowMultiple && !isActive) {
        faqItems.forEach(otherItem => {
          if (otherItem !== item) {
            otherItem.classList.remove('active');
            otherItem.querySelector('.faq-question').setAttribute('aria-expanded', 'false');
          }
        });
      }

      // Toggle current item
      if (isActive) {
        item.classList.remove('active');
        question.setAttribute('aria-expanded', 'false');
      } else {
        item.classList.add('active');
        question.setAttribute('aria-expanded', 'true');
      }
    });
  });
})();
</script>

